	var result = dbConn.executeUpdate(&quot;INSERT INTO AWS_FILE_MAP (date_submitted, file_type, file_name, aws_file_id) VALUES (NOW(),&apos;ccda&apos;, &apos;&quot;+$(&apos;originalFilename&apos;)+&quot;&apos;, &apos;&quot;+awsFileId+&quot;&apos;)&quot;);

